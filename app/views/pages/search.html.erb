<div id="content">

  <h4>
    <%= @pages.total_entries %> <%= render :partial => 'search_form' %> pages found
  </h4>

<table>
<% @pages.each do |page| %>
  <tr>
    <td><%= render :partial => 'pages/page', :locals => { :page => page } %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @pages %>

</div>
