window.reading ||= {}

reading.curl =
  apiContext: window.reading
  defineContext: window.reading
  baseUrl: "//<%= DOMAIN %>/assets"
  packages:
    app:
      path: "backbone"
      main: "reading"
    extend:
      path: "libs/extend"
