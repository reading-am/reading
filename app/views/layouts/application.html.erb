<!DOCTYPE html>
<html>
<%= render :partial => 'layouts/head' %>
<body id="application_layout" class="r_<%= signed_in? ? 'signed_in' : 'signed_out' %>">
  <%= render :partial => 'layouts/fb_api' %>
  <%= render :partial => 'layouts/titlecard' %>
  <div class="container">
    <%= render :partial => 'layouts/header' if params[:action] != 'visit' %>
    <%= yield %>
  </div>
  <%= render :partial => 'layouts/loading' %>
  <%= render :partial => 'layouts/quantcast' if Rails.env == 'production' %>
</body>
</html>
