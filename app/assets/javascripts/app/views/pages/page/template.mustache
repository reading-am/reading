<div class="r_page">
  <div class="r_page_{{medium}} {{#embed}}r_has_embed{{/embed}}">
    {{#embed}}<a href="{{url}}">{{{embed}}}</a>{{/embed}}
    <h2>
      <a href="{{url}}" target="_blank">
        <span>{{title}}</span>
      </a>
    </h2>
    <p class="description" title="{{description}}">
      {{description}}
    </p>
  </div>
</div>
